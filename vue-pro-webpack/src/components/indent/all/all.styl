bg($url)
    background url($url + "@2x.png")  no-repeat center/cover
    @media (-webkit-min-device-pixel-ratio: 3),(min-device-pixel-ratio:3)
        background url($url + "@3x.png")  no-repeat center/cover
.indent{
    color: #525252;
    background: #f1f3ff ;
      header{
          a{
           color: white !important;
		  splay: inline-block;
          i{
            font: 700 16px/34px simsun
           }
        }
		.router-link-active {
			color: #007aff;
		}	
	}
    .mint-header{
			background: black;
			position: fixed;
			top: 0;
			left:0;
			width: 100%;
			z-index: 13;
			height: 0.8rem;
		}
        .mui-bar{
			position: relative;
			box-shadow: none;
			background: white;
			height: 0.8rem;
            margin-top: 0.8rem;
            .router-link-active span::before{
                content:'';
                height: 2px;
                position: absolute;
                bottom: -1px;
                left: 0;
                width: 100%;
                background: linear-gradient( 90deg,#41cc6e, #35b05d)
            }
            .mui-tab-item{
                padding: 0 0.2rem;
                width: 20%;
                float: left;
                text-align: center;
                height: 100%;
                text-decoration: none;
                span{
                    font-family: '微软雅黑';
                        background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#41cc6e), to(#35b05d));
                        -webkit-background-clip: text;
                        -webkit-text-fill-color:transparent;
                    position: relative;
                    display: block;
                    color: #41cc6e;
                    font-size: 12px;
                    line-height: 0.8rem;    
                }
            }
		}
        .program{
            padding-bottom:0.6rem; 
            .item{
                margin-top: 0.1rem;
                div:nth-child(1){
                    height 0.8rem;
                    line-height 0.8rem;
                    padding: 0 0.2rem;
                    background: white;
                    
                    s{
                        width 0.3rem;
                        height 0.3rem;
                        display: inline-block;
                        background: url(../../../assets/img/touxiang_01.png) no-repeat center/cover
                        vertical-align: middle;
                        border: 1px solid #dbdbdb;
                     }
                     span{
                        vertical-align: middle;
                      }
                     span:nth-child(2){
                         font-size: 12px;
                         margin-left 0.1rem;                         
                     }
                     span:nth-child(3){
                        display: inline-block;
                        height 0.12rem
                        width 0.12rem
                        margin-left 0.08rem
                        bg('../../../assets/img/more')
                     }
                     span:nth-child(4){
                        font-size: 12px;
                        color: #f5841f;
                        float: right;
                     }
                }
                div:nth-child(2){
                  background: white;
                //   height 1.5rem;
                  padding: 0.2rem 0.2rem 0.1rem;
                  
                  i{
                     float: left;
                     width 1.2rem
                     height 0.9rem
                     overflow: hidden;
                     background: url(../../../assets/img/baoming-6@2x.png) no-repeat center/cover
                   }
                   .msg{
                       padding: 0;
                       overflow: hidden;
                       padding-left: 0.2rem;
                       h4{
                           font-size: 12px;
                           float: left;
                        }
                        span{
                            float: right;
                            margin: 0;
                            font-size: 14px;
                            color: #3b3b3b;
                            font-weight: 700;
                         }
                         div{
                            padding: 0;
                            font-size: 12px;
                            margin-top: 0.4rem;
                          }
                   }
                   
                }
                div:nth-child(3){
                    background: white;
                    margin-top: 0.02rem;
                    padding: 0.15rem 0.2rem;
                    height 0.8rem;
                    
                    span{
                        float: right;
                        font-size: 10px;
                        width 1.54rem;
                        text-align: center;
                        padding: 0.1rem  0;
                        border-radius:0.3rem
                        margin-left 0.2rem;
                     }
                     .pay{
                        color: white;
                        background: #f5841f;
                        border: 1px solid #f5841f;
                     }
                     .cancel{
                         border: 1px solid #f5841f;
                         color: #f5841f;
                     }
                     .consult{
                         border: 1px solid #35b05d;
                         color: #35b05d;
                     }
                }
            }
        }
        .reimburse{
            display: none;
            position: absolute;
            top: 0;
            // transform :scale(0)
            height 100%
            transition: all .3s;
            width 100%
            background: rgba(0,0,0,.4);
            >div{
                border-radius: 0.2rem;
                overflow: hidden;
                position: absolute;
                top: 50%;
                left: 50%;
                width 5.4rem
                height 3.3rem
                margin-top:-1.65rem;
                margin-left:-2.7rem;
                background: white;
                // transform: translateY(-1000px)
                // transition: all 1s;
                div:nth-child(1){
                    height 2.5rem;
                    padding-top: 0.6rem;
                    h4{
                        font-size: 17px;
                        color: #35b05d;
                        text-align: center;
                        font-weight: 700;
                     }
                     p{
                         margin-top: 0.3rem;
                        text-align: center;
                        font-size: 12px;
                       }
                     span{
                        display: block;
                        font-size: 11px;
                        text-align: center;
                        color: #999;  
                        margin-top: 0.1rem;                      
                      }  
                }
                div:nth-child(2){
                    overflow: hidden;
                    span{
                        float: left;
                        height 0.8rem
                        line-height 0.8rem
                        text-align: center;
                        width 50%
                        font-weight: 600;
                        font-size: 15px;
                        vertical-align: middle;
                     }
                     span:nth-child(1){
                         color:  #35b05d;
                         background: #fafbfe;
                     }
                     span:nth-child(2){
                        background: linear-gradient(-90deg,#35b05d, #41cc6e);
                        color: white;
                     }
                 } 
            }
        }
}