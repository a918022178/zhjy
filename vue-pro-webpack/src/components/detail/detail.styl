bg-image($url)
    background url($url + "@2x.png") no-repeat center/cover
    @media (-webkit-min-device-pixel-ratio: 3),(min-device-pixel-ratio:3)
        background url($url + "@3x.png") no-repeat center/cover
		.divide{
			height: 5px;
		}
		*{
			// line-height: 20px
		}
		h4{
			font-family: "MicroSoft YaHei";
			font-weight: 700;
			font-size: 13px;
		}
		.title1{
			padding: 0.6rem 0.2rem;
			background: white;

			.free{
				overflow: hidden
			}
			.scoe{
				font-size 11px;
				// line-height: 11px;
				.join{
					vertical-align: middle;
					// display: inline-block;
					margin-left 20px
				}
			}

			.common-star{
				vertical-align: middle;
				.star-item{
				
				}
			}
				div:nth-child(3){
				overflow: hidden;
			}
			    div:nth-child(3) ul{
				display: flex;
				justify-content: space-between;
				margin-top:0.58rem;
				margin-bottom 0.04rem;
				li{
					flex:1;
					font-size: 11px!important;
					i{
						vertical-align: middle;
						margin-right: 0.05rem;
						display: inline-block;
						width: 0.34rem;
						height: 0.34rem;
						background-repeat:no-repeat;
					}
				}
				li:nth-child(1) i{
					bg-image('../../assets/img/label_01')
				}
				li:nth-child(2) i{
					bg-image('../../assets/img/label_02')
				}
				li:nth-child(3) i{
					bg-image('../../assets/img/label_03')
				}
				li:nth-child(4) i{
					bg-image('../../assets/img/label_05')
				}
			}
			div:nth-child(3) ul li{
				font-size: 14px;
				color: #525252;
			}
			h4{
				float:left
				font-size: 15px;
				color: #3b3b3b;
				vertical-align: middle;
				margin-left 0.1rem
				margin-top -0.04rem

			}
			.free{
				margin-bottom: 0.32rem;
			}
			div:nth-child(1) span{
				float:left
				width: 0.58rem;
				height: 0.32rem;
				bg-image('../../assets/img/title_free')
				vertical-align: middle;

			}
			div:nth-child(2) span:nth-child(2){
				font-size: 11px;
			}
		}
		.pics{
			overflow:hidden;
		}
