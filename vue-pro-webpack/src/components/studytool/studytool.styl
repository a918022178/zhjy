	 $shadow=0px 16px 16px rgb(229,223,255,0.1);
     bg($url)
        background url($url + "@2x.png") no-repeat center/cover
        @media (-webkit-min-device-pixel-ratio: 3),(min-device-pixel-ratio:3)
          background url($url + "@3x.png") no-repeat center/cover;
.tool{
     font-family: 'MicroSoft YaHei';
     color: #525252;
     position: relative;
    .seek_box{
        position: fixed
        height 0
        width 100%
        top: 0;
        left 0;
        margin-top: 1.6rem
        z-index: 4;
        .seek{
            padding 0.4rem   0.2rem
            transition: all .5s;
            position: absolute
            top: -0.17rem;
            transform: translateY(-6rem)
            // left: 0;
            height 6rem;
            width 100%
            background: white;;
            z-index: 4;
           
            h4{
                font-weight: bold;
                margin: .4rem 0
             }
             span{
                 display: inline-block;
                 padding: 0 0.4rem
                 height 0.6rem
                 line-height 0.6rem
                 border-radius: 0.5rem
                 text-align center
              }
              .unactive{
                  color: #999;
                  border 1px solid #999
              }
              .active{
                color: white;
                background: linear-gradient(-90deg, #35b05d, #41cc6e);
                border 1px solid transparent;
              }
              .btn{
                  margin-top: .6rem;
                  span{
                    width 2.8rem
                   }
              }
            }
        }
     header a{
            color: white !important;
        }
	header a i{
        font: 700 16px/34px simsun;
        }
    .mint-header{
        background: black;
        position: fixed;
        top: 0;
        left:0;
        width: 100%;
        z-index: 13;
        height 0.8rem
        }

		.router-link-active {
		}
		.mui-bar{
			position: relative;
			box-shadow: none;
			background: white;
			height: 0.8rem;
            margin-top: 40px;
            padding: 0 0.2rem;
            padding-right 1rem;
            z-index: 6;
            .tab::-webkit-scrollbar {
                    display: none;
                }
            .tab{
                width 5.2rem
                overflow-x: scroll
                div{
                    width 5.6rem
                 }
              }
            
            .search{
                position: absolute;
                height 100%
                width 1rem;
                box-shadow: -7px -2px 12px rgb(229,223,255,0.1);
                right: 0;
                top: 0;
                background: white;
                text-align: center;
                padding-top: 0.2rem;
                i{
                    display: inline-block;
                    height 0.4rem
                    width 0.4rem
                    bg('../../assets/img/shaixuan-icon')
                 }
             }
        .router-link-active span{
            background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#35b05d), to(#41cc6e));
				-webkit-background-clip: text;
				-webkit-text-fill-color:transparent;
        }    
		.router-link-active span::before{
                content: '';
                position: absolute;
                bottom: 0;
                left: 0;
                width 100%
                height 2px;
                background: linear-gradient( 90deg,#41cc6e, #35b05d)
            }
		}
		.mui-tab-item{
			padding: 0 0.05rem;
            padding-bottom 2px;
            position: relative;
			width: 20%;
			float: left;
			text-align: center;
			height: 100%;
			text-decoration: none;
			
            span{
                display: block;
                position: relative;
                font-size: 15px;
                line-height: 0.8rem;
                font-family: '微软雅黑';
				background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#9b9b9b), to(#6b6b6b));
				-webkit-background-clip: text;
				-webkit-text-fill-color:transparent;
            }    
		}
        .main{
            margin-top: 0.1rem;
            .item:nth-child(1) i{
                background: url('../../assets/img/logo_1.png') no-repeat center/cover
             }
            .item:nth-child(2) i{
                background: url('../../assets/img/logo_2.png') no-repeat center/cover
             }
            .item:nth-child(3) i{
                background: url('../../assets/img/logo_3.png') no-repeat center/cover
             }
            .item{
                
                .summary{
                    transition: all .5s;
                    position: relative;
                    overflow: hidden;
                    margin-bottom: 5px;
                    line-height: 22px;
                    word-break: break-all;
                    font-size: 12px;
                    line-height: 0.4rem;
                    p{
                        margin-top: 0.12rem;
                        font-size: 12px;
                        line-height: 0.4rem;
                        display: inline-block;
                        transition: all .5s;
                     }
                }
                .packup p{
                    height: 0.7rem;
                    overflow: hidden;
                }
                .packup:before{
                     display: block;
                    content: attr(data-content);
                    position: absolute;
                    z-index: 1;
                    left: 0;
                    top: 0;
                    height: 0.4rem;//这里的高是3×22的结果，其中22是行高 ，3是4-1
                    width: 100%;
                    overflow: hidden;
                    // color: #000;
                    background-color: #fff;
                    // text-indent: 5em;
                    margin-top: 0.12rem;
                    font-size: 12px;
                    line-height: 0.4rem;
                }
                .packup:after{
                    display: -webkit-box;
                    -webkit-box-orient: vertical;
                    -webkit-box-sizing: border-box;
                    box-sizing: border-box;
                    -webkit-line-clamp: 2;//4就是需要在收起的时候显示的行数
                    content: attr(data-content);
                    position: absolute;
                    left: 0;
                    top: 0;
                    width: 100%;
                    height: 100%;
                    text-indent: -6em;
                    padding-right: 6em;
                    // color: #000;
                    background-color: #fff;
                    margin-top: 0.12rem;
                    font-size: 12px;
                    line-height: 0.4rem;
                }
                margin-bottom 0.02rem;
                background: white;
                padding: 0.6rem 0.4rem 0.6rem 0.2rem;
                i{
                    height 1.5rem
                    width 1.5rem
                    float left
                    overflow: hidden;
                    margin-top: 3px;
                 }
                 .right{
                     overflow: hidden;
                     padding-left 0.4rem;
                     .fl{
                         h4{
                             font-size: 15px;
                             color: #3b3b3b;
                             font-weight: bold;
                          }
                         h5{
                            font-size: 13px;
                            margin-top: 0.08rem;
                          }
                     }
                     .fr{
                         width 1.24rem
                         height 0.44rem
                         line-height: 0.4rem
                         text-align: center
                          font-size: 12px;
                          color: #35b05d;
                          border 1px solid #35b05d ;
                          border-radius: 0.2rem;
                          margin-top: 0.1rem;
                     }
                     p{
                        
                      }
                      span{
                            font-size: 11px;
                            color: #9b9b9b;
                            position: absolute;
                            right: 0;
                            bottom: 4px;
                            z-index: 3;
                            s{
                                width 0.25rem
                                height 0.25rem
                                display: inline-block;
                                bg('../../assets/img/shouqi-icon')
                                vertical-align: middle
                                margin-bottom 1px;
                                margin-left 0.1rem;
                             }
                             .change{
                                 bg('../../assets/img/zhankai-icon');
                             }
                         }
                 }
            }
        }
}